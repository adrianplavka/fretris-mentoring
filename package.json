{
  "name": "fretris-mentoring",
  "version": "2.0.0",
  "description": "tetris",
  "main": "index.js",
  "keywords": [
    "git",
    "management"
  ],
  "author": "Adrian Plavka",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFilesAfterEnv": ["./node_modules/jest-enzyme/lib/index.js"],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "verbose": true,
    "silent": true
  },
  "scripts": {
    "build": "webpack --config src/config/webpack.common.js && webpack --config src/client/web/config/webpack.prod.js",
    "start": "node src/.dist/index.js",
    "build:server": "webpack --config src/config/webpack.common.js",
    "start:server": "webpack --config src/config/webpack.dev.js --watch",
    "build:client": "webpack --config src/client/web/config/webpack.prod.js",
    "start:client": "webpack-dev-server --open --config src/client/web/config/webpack.ws.dev.js",
    "test": "jest",
    "test:server": "jest --testPathIgnorePatterns=/client/",
    "test:client": "jest client.web"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.9",
    "@types/express": "^4.0.37",
    "@types/hammerjs": "^2.0.35",
    "@types/history": "^4.6.2",
    "@types/jest": "^21.1.1",
    "@types/jquery": "^3.2.16",
    "@types/lodash": "^4.14.76",
    "@types/node": "^8.0.31",
    "@types/react-dom": "^15.5.5",
    "@types/react-redux": "^5.0.9",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/redux-logger": "^3.0.4",
    "@types/socket.io": "^1.4.31",
    "@types/socket.io-client": "^1.4.32",
    "@types/superagent": "^3.5.5",
    "@types/supertest": "^2.0.3",
    "babel-minify-webpack-plugin": "0.3.1",
    "clean-webpack-plugin": "1.0.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-cli": "^24.1.0",
    "jest-enzyme": "^4.0.0",
    "nodemon-webpack-plugin": "^4.0.7",
    "raf": "^3.3.2",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.18.2",
    "superagent": "^3.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "5.3.3",
    "typescript": "^2.5.3",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "express": "^4.16.1",
    "hammerjs": "^2.0.8",
    "history": "^4.7.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  }
}
